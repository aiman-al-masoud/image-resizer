{
    "short_name": "hello",
    "name": "Buruf",
    "icons": [
        {
            "src": "/static/favicon.wepb",
            "type": "image/webp",
            "sizes": "260x280"
        }
    ],

    "share_target": {
        "action": "/file-collector",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "name",
            "text": "description",
            "url": "link",
            "files": [
                {
                    "name": "lists",
                    "accept": [ "text/csv", ".csv" ]
                },
                {
                    "name": "photos",
                    "accept": [ "image/jpeg", ".jpg" ]
                }
            ]
        }
    },

    "start_url": "/",
    "background_color": "#ffffff",
    "display": "standalone",
    "theme_color": "#ffffff"
}